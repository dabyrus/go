package main

import (
	"fmt"
	"sort"
)

func main() {
	arr := []int{3, 15, 2, 9}

	// Отсортировать и взять последний элемент
	sort.Ints(arr)
	// Вывод: 15
	fmt.Println(arr[len(arr)-1])

	// Или через цикл
	max := arr[0]
	for _, element := range arr {
		if element > max {
			max = element
		}
	}

	// Вывод: 15
	fmt.Println(max)
}
